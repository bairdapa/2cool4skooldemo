<div class="container">
	<div class="create_review_title">
		<h1 class="create_review_title">Create Professor</h1>
	</div>

	<div class="create_professor_form">
		<a class="normal_text">First name</a>
		<input type="text" id="create_prof_fname"><br><br>
		<a class="normal_text">Last name</a>
		<input type="text" id="create_prof_lname"><br><br>

		<label for="world" class="normal_text">World</label>
		<select name="world" id="create_prof_world">
			{{#each worlds}}
				<option value="{{worldId}}">{{worldName}}</option>
			{{/each}}
		</select><br><br>

		<label for="school" class="normal_text">School</label>
		<select name="school" id="create_prof_school">
			{{#each schools}}
				<option value="{{schoolId}}">{{schoolName}}</option>
			{{/each}}
		</select><br><br>

		<a class="normal_text">Picture URL</a>
		<input type="text" id="create_prof_pic"><br><br>

		<button id="create_prof_submit">Create Professor</button> 
	</div>



</div>
